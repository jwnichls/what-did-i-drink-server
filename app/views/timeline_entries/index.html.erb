<h1>Listing timeline_entries</h1>

<table>
  <tr>
    <th>User</th>
    <th>Drink</th>
    <th>Type</th>
    <th>Description</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @timeline_entries.each do |timeline_entry| %>
  <tr>
    <td><%= timeline_entry.user_id %></td>
    <td><%= timeline_entry.drink_id %></td>
    <td><%= timeline_entry.type %></td>
    <td><%= timeline_entry.description %></td>
    <td><%= link_to 'Show', timeline_entry %></td>
    <td><%= link_to 'Edit', edit_timeline_entry_path(timeline_entry) %></td>
    <td><%= link_to 'Destroy', timeline_entry, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Timeline entry', new_timeline_entry_path %>
